{
  "version": "2",
  "sql": [
    {
      "engine": "postgresql",
      "schema": "./migrations/*.sql",
      "queries": "./queries/*.sql",
      "gen": {
        "go": {
          "package": "services",
          "out": "./internal/services",
          "emit_json_tags": true
        }
      }
    }
  ]
}
